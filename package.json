{
  "name": "@renovatebot/pgp",
  "version": "0.0.0-semantic-release",
  "description": "PGP library for renovate",
  "keywords": [
    "renovate",
    "pgp"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/renovatebot/pgp.git"
  },
  "license": "Apache-2.0",
  "author": "Michael Kriese <michael.kriese@gmx.de>",
  "type": "module",
  "files": [
    "README.md",
    "LICENSE",
    "index.js",
    "index.d.ts",
    "dist/"
  ],
  "scripts": {
    "build": "./gradlew build && dotnet publish dotnet && node tools/patch-build.js",
    "debug:teavm-wasm": "node --inspect-brk test/teavm/wasm.js",
    "prepare": "husky",
    "prettier": "PRETTIER_EXPERIMENTAL_CLI=1 prettier -c -u .",
    "prettier:fix": "PRETTIER_EXPERIMENTAL_CLI=1 prettier -w -u .",
    "test": "vitest run",
    "test:teavm-wasm": "node test/teavm/wasm.js",
    "test:teavm-wasm:prof": "node --prof test/teavm/wasm.js"
  },
  "devDependencies": {
    "@containerbase/semantic-release-pnpm": "1.3.2",
    "@types/node": "22.18.13",
    "@vitest/coverage-v8": "4.0.4",
    "conventional-changelog-conventionalcommits": "9.1.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.6",
    "markdownlint-cli2": "0.18.1",
    "prettier": "3.6.2",
    "prettier-plugin-packagejson": "2.5.19",
    "semantic-release": "25.0.1",
    "typescript": "5.9.3",
    "vitest": "4.0.4"
  },
  "packageManager": "pnpm@10.20.0",
  "engines": {
    "node": "^22.11.0 || >=24.10.0",
    "pnpm": "^10.0.0"
  }
}
