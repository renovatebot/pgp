    "markdownlint-cli2": "0.18.1",
    "prettier": "3.6.2",
    "prettier-plugin-packagejson": "2.5.19",
    "typescript": "5.8.3",
    "vitest": "3.2.4"
  },
  "packageManager": "pnpm@10.19.0",
  "engines": {
    "node": "^22.11.0 || >=24.10.0",
    "pnpm": "^10.0.0"
